{
  "expo": {
    "name": "DreamToon",
    "slug": "DreamToon",
    "version": "1.0.3",
    "orientation": "portrait",
    "icon": "./assets/images/ToonIcon.png",
    "userInterfaceStyle": "dark",
    "scheme": "dreamtoon",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.minks.dreamtoon",
      "developmentTeam": "67AJ8B7RAP",
      "googleServicesFile": "./GoogleService-Info.plist",
      "buildNumber": "24",
      "infoPlist": {
        "NSMicrophoneUsageDescription": "To record your voice as you describe a dream. The app then transcribes your recording to generate your comic panels.",
        "NSCameraUsageDescription": "To let you set a profile pictur to use as inspiration for a new comic panel.",
        "NSPhotoLibraryUsageDescription": "To pull photos for your profile or to save the comic panels you create directly to your photo library, allowing you to easily view and share them.",
        "ITSAppUsesNonExemptEncryption": false,
        "UIBackgroundModes": ["fetch"]
      },
      "appStoreUrl": "https://apps.apple.com/app/dreamtoon/id6747994787",
      "appleTeamId": "67AJ8B7RAP"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.anonymous.mobile",
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.1075347854434-7rmoljgc77itobhjoi18ukgdfg57g690"
        }
      ],
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ],
      "expo-router",
      [
        "expo-notifications",
        {
          "icon": "./assets/images/notification-icon.png",
          "color": "ffffff"
        }
      ],
      "expo-media-library",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "f90a1f67-a1d1-4942-ab7e-f984e2bf717e"
      }
    },
    "description": "Transform your dreams into stunning comic panels with AI-powered storytelling"
  }
}
